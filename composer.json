{
	"name": "bdhabib/laravel-location",
	"description": "This is laravel location liberty ",
	"keywords": ["Laravel","Location","BD Location", "Bangladesh", "Creativehabib"],
	"type": "library",
	"license": "MIT",
	"version": "1.0.3",
	"authors": [
		{
			"name": "Habibur Rahaman",
			"email": "shamolrahaman@gmail.com",
			"role": "Developer"
		}
	],
	"minimum-stability": "stable",
	"autoload": {
		"psr-4": {
			"Bdhabib\\Location\\": "src/"
		}
	},

	"autoload-dev": {
		"psr-4": {
			"Bdhabib\\Location\\Test\\": "tests"
		}
	},
	"scripts": {
		"test": "vendor/bin/phpunit tests"
	},
	"extra": {
		"laravel": {
			"providers": [
				"Bdhabib\\Location\\LocationServiceProvider"
			]
		}
	},
	"require": {
		"php": "^8.2",
		"illuminate/support": "^12.6.0"
	},
	"require-dev": {
		"mockery/mockery": "^1.6.10",
		"orchestra/testbench": "3.8.*| 10.0.*",
		"phpunit/phpunit": "^11.0",
		"illuminate/contracts": "5.8.15|^6.0|^12.0",
		"illuminate/filesystem": "^5.8.15|^6.0|^12.0",
		"illuminate/database": "^5.8.15|^6.0|^12.0",
		"phpunit/php-code-coverage": "^11.0@dev"

	}
}
